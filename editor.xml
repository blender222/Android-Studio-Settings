<application>
  <component name="AndroidFoldingSettings">
    <option name="collapseAndroidStrings" value="false" />
    <option name="COLLAPSE_ANDROID_TEXT" value="false" />
  </component>
  <component name="CodeInsightSettings">
    <option name="completionCaseSensitive" value="2" />
    <option name="COMPLETION_CASE_SENSITIVE" value="2" />
    <option name="SMART_END_ACTION" value="false" />
  </component>
  <component name="CodeVisionSettings">
    <option name="codeVisionGroupToPosition">
      <map>
        <entry key="inheritors" value="Default" />
        <entry key="problems" value="Default" />
        <entry key="references" value="Default" />
        <entry key="vcs.code.vision" value="Default" />
      </map>
    </option>
  </component>
  <component name="EditorSettings">
    <option name="languageBreadcrumbsMap">
      <map>
        <entry key="XML" value="true" />
      </map>
    </option>
    <option name="USE_SOFT_WRAPS" value="PREVIEW" />
    <option name="STRIP_TRAILING_SPACES" value="None" />
    <option name="IS_RIGHT_MARGIN_SHOWN" value="false" />
    <option name="SHOW_BREADCRUMBS_ABOVE" value="true" />
    <option name="IS_ANIMATED_SCROLLING" value="false" />
  </component>
  <component name="InlayHintsSettings">
    <option name="disabledHintProviderIds">
      <set>
        <option value="Groovy.groovy.variable.type.hints" />
        <option value="JAVA.java.implicit.types" />
        <option value="kotlin.kotlin.call.chains.hints" />
        <option value="kotlin.kotlin.lambdas.hints" />
        <option value="kotlin.kotlin.references.types.hints" />
      </set>
    </option>
    <option name="lastViewedProviderKeyId" value="kotlin.parameter.hints.old" />
    <settingsMapElement>
      <kotlin.kotlin.values.hints>
        <Settings>
          <option name="ranges" value="false" />
        </Settings>
      </kotlin.kotlin.values.hints>
      <kotlin.kotlin.lambdas.hints>
        <Settings>
          <option name="implicitReceiversAndParams" value="false" />
          <option name="returnExpressions" value="false" />
        </Settings>
      </kotlin.kotlin.lambdas.hints>
    </settingsMapElement>
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
  </component>
</application>